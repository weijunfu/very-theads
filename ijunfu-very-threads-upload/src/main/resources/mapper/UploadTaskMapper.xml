<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ijunfu.upload.mapper.UploadTaskMapper">
    <resultMap id="BaseResultMap" type="com.ijunfu.upload.entity.UploadTask">
        <id column="task_id" property="taskId"/>
        <result column="file_name" property="fileName"/>
        <result column="part_nums" property="partNums"/>
        <result column="file_md5" property="fileMd5"/>
        <result column="file_path" property="filePath"/>
        <result column="del_status" property="delStatus"/>
    </resultMap>

    <sql id="Base_Column_List">
        task_id, file_name, part_nums, file_md5, file_path, del_status
    </sql>


</mapper>